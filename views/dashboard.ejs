
<div class="bg-dash">
    <%- include('partiallink/dashheader');-%>
    
    
	<p class="profile-title">Hej, <span id="name-cc"><%= user.name %></span></p>
	<div class="msgdisplay">
		<%- include('partials/messages');-%>

	</div>
	
	
    <div class="profile">
		<div class="col-md-2">
			<div class="profile-sidebar">
				<!-- SIDEBAR USERPIC -->
				<div class="profile-userpic">
					<img src="source/userProf.jpg" class="img-responsive" alt="">
				</div>
				<!-- END SIDEBAR USERPIC -->
				<!-- SIDEBAR USER TITLE -->
				<div class="profile-usertitle">
					<div class="profile-usertitle-name">
						<%= user.name%> / <%= user.Lastname%>
					</div>
				</div>
				<!-- END SIDEBAR USER TITLE -->
				<!-- SIDEBAR BUTTONS -->
				<div class="profile-userbuttons">
					<button type="button" class="btn btnEdit btn-sm">Redigera</button>
					<a href="/users/logout" class="btn btnBooklist btn-sm" id="logoutbtn"> Loga ut <i class="fas fa-long-arrow-alt-right"></i></a>
				</div>
				<!-- END SIDEBAR BUTTONS -->
				<!-- SIDEBAR MENU -->
				<div class="profile-usermenu">
					
						
							<a href="/dashboard" id="menus-link" class="active">
							Mina bokningar </a>
						
                        <a href="/dashboard" id="menus-link">
						
                        Information </a>
						
							<a href="/dashboard" id="menus-link">
							Inställning </a>
						
					
							<a href="/dashboard" target="_blank" id="menus-link">
							Hälp </a>
				
				
				</div>
				<!-- END MENU -->
			</div>
		</div>

		<div class="col-md-9">
			<div class="profile-content">
				<div class="bookningInfo">
					<h1 class="title-bookningInfo">Information om dina bokningar.</h1>
					<div class="container-table100">
						<div class="wrap-table100">
							<div class="table100 ver3 m-b-110">
								<div class="table100-head">
									<table>
											<tr class="row100 head">
												<th></th>
												<th>Namn</th>
												<th>Tid & datum</th>
												<th>Telefonnummer</th>
												<th>E-postadress</th>
												<th style="display: none;"></th>
											</tr>
									</table>
								</div>
				
								<div class="table100-body js-pscroll ps ps--active-y">

								<form action="/deleteUserAP" method="POST">
										<table>
											<tbody>
												
												<% for(let i = 0; i < item.length; i++){ %>
												 <tr class="Rownm rowOnClick">
													
													<% if(item[i]){ %>
														<% if(item[i].timeAP || item[i].nameAP || item[i].emailAP || item[i].numberAP  || item[i].idAP){ %>
														  <td id="existent"><%= i+1%></td>
														  <td><%= item[i].nameAP %> / <%= item[i].lastnameAP %></td>
														  <td id="mentionCC"><%= item[i].timeAP %> ( <%= item[i].dateAP %> ) </td>
														  <td><%= item[i].numberAP %></td>
														  <td><%= item[i].emailAP %></td>
														  <td style="display: none;" class="idTakenFrom"> <input id="idTakenFrom"+i name="uniqueid<%= i %>" type="text" value="<%= item[i]._id %>"></td>
														  <td style="display: none;" > <input id="bool<%= i %>" type="text" name="uniqueid<%= i %>" type="text" value="false"></td>
														  <%}} else{%>
															
														<%}%>
															
													</tr>
												<%}%>
														
														
														
													</tbody>
												</table>
												<div class="edits-btn-AP">
													<!-- <input id="transferValueForDeleteBtn" name="uniqueid" style="display: none;" value="" /> -->
													<a id="open-warning-btn-delete">Ta bort</a>
												</div>
												<div class="warning-delete-ap">
													<div class="bgw-warning">
														<h1>Are you sure you want to delete the appointment?</h1>
														<div class="icon-warning"></div>
														<div class="btnsss">
															<button id="deleteBtn" class="deleteBtn" type="submit" name="delete" value="delete" style="display: block;"><i class="fas fa-trash-alt"></i><span> Ja</span></button>
															<a id="cancelBtn">Nej</a>
														</div>	
													</div>
												</div>
										</form>
							</div>
				
						</div>
					</div>
					
				</div>
				</div>
				<h1 style="display: none;" id="ifNoAP"> <span id="userNameDisplay"> <i class="fas fa-exclamation-circle"></i></span> <span id="mellanDivider"> | </span> <span> Du har ingen bokade tider.<br> Vill du boka en tid hos oss?</span></h1>
				<div class="btns-link-AP">
					<a style="display: none;" href="/table" id="btn-link-AP">Boka en tid <i class="fas fa-long-arrow-alt-right"></i></a>
					<a style="display: block;" href="/table" id="btn-link-AP-s">Boka en tid. <i class="fas fa-long-arrow-alt-right"></i></a>
				</div>
		</div>
	</div>

</div>

<script type="text/javascript" src="apps/script.js"></script>